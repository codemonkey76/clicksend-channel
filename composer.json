{
    "name": "laravel-notification-channels/clicksend",
    "description": "ClickSend Notification Channel",
    "homepage": "https://laravel-notification-channels.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Shane Poppleton",
            "email": "shane@bjja.com.au",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "clicksend/clicksend-php": "^5.0",
        "codemonkey76/laravel-clicksend": "^0.4.0",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0 || ~9.0 || ~10.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0 || ~9.0 || ~10.0",
        "laravel/framework": "~6.0 || ~7.0 || ~8.0 || ~9.0 || ~10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\ClickSend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\ClickSend\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra" : {
        "laravel": {
            "providers": [
                "NotificationChannels\\ClickSend\\ClickSendServiceProvider"
            ]
        }
    }
}
